{
  "title": "rx-hub",
  "gitbook": ">3.0.0",
  "language": "zh-hans",
  "plugins": [
    "-lunr",
    "-search",
    "-highlight",
    "-livereload",
    "search-plus@^0.0.11",
    "simple-page-toc@^0.1.1",
    "github@^2.0.0",
    "github-buttons@2.1.0",
    "edit-link@^2.0.2",
    "disqus@^0.1.0",
    "prism@^2.1.0",
    "prism-themes@^0.0.2",
    "advanced-emoji@^0.2.1",
    "anchors@^0.7.1",
    "include-codeblock@^3.0.2",
    "ace@^0.3.2",
    "emphasize@^1.1.0",
    "katex@^1.1.3",
    "splitter@^0.0.8",
    "mermaid@^0.0.9",
    "tbfed-pagefooter@^0.0.1",
    "expandable-chapters-small@^0.1.7",
    "sectionx@^3.1.0",
    "local-video@^1.0.1",
    "anchor-navigation-ex@0.1.8",
    "favicon@^0.0.2",
    "todo@^0.1.3",
    "edit-link"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "disqus": {
      "shortName": "gitbookuse"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
      ]
    },
    "github": {
      "url": "https://github.com/ccqgithub/rx-hub"
    },
    "github-buttons": {
      "repo": "ccqgithub/rx-hub",
      "types": [
        "star"
      ],
      "size": "small"
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "edit": true
    },
    "sharing": {
      "weibo": true,
      "facebook": true,
      "twitter": true,
      "google": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "google",
        "twitter",
        "weibo",
        "instapaper"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright © season.chen",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "edit-link": {
      "base": "https://github.com/ccqgithub/rx-hub/tree/master/docs",
      "label": "Edit This Page"
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    },
    "sectionx": {
      "tag": "b"
    }
  },
  "links": {
    "sharing": {
      "facebook": false,
      "twitter": false
    }
  }
}
